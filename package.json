{
  "name": "gulp-base-project",
  "version": "",
  "repository": {
    "type": "git",
    "url": "gulp-base-project"
  },
  "author": "",
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "gulp-cache": "^0.4.5",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^3.0.1",
    "gulp-exif": "^0.10.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev-all": "^0.8.24",
    "gulp-sass": "^2.3.2",
    "gulp-scss-lint": "^0.4.0",
    "gulp-size": "^2.1.0",
    "gulp-uglify": "^1.5.4",
    "main-bower-files": "^2.13.1"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "indent": [
        2,
        2
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": [
        2,
        {
          "skipBlankLines": true
        }
      ],
      "no-unused-vars": 0
    },
    "globals": {
      "jQuery": false,
      "$": false,
      "_": false
    }
  },
  "dependencies": {}
}
